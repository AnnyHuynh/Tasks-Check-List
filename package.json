{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node app.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "axios": "^0.18.0",
    "if-env": "^1.0.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongod": "^2.0.0",
    "mongoose": "^5.3.4",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.10",
    "path": "^0.12.7",
    "pug": "2.0.0-beta11"
  }
}
